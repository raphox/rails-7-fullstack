    "alpinejs": "^3.10.4",
    "autoprefixer": "^10.4.12",
    "esbuild": "^0.15.12",
    "lodash": "^4.17.21",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1"
  },
  "scripts": {
